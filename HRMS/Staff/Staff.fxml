<AnchorPane fx:id="Context" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HRMS.Staff.StaffController">
   <children>
      <ImageView fx:id="staffback" fitHeight="23.0" fitWidth="35.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#BackOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../ClientSelecting/back-arrow.png" />
         </image>
      </ImageView>
      <Button fx:id="booking" layoutX="60.0" layoutY="37.0" mnemonicParsing="false" onAction="#showOnAction" prefHeight="31.0" prefWidth="74.0" text="Bookings" textFill="#807d91" />
      <TableView fx:id="orderDetailsTable" layoutX="60.0" layoutY="80.0" prefHeight="300.0" prefWidth="480.0">
         <columns>
            <TableColumn fx:id="usernameColumn" prefWidth="92.79998779296875" text="Username" />
            <TableColumn fx:id="animalTypeColumn" prefWidth="83.20001220703125" text="Animal Type" />
            <TableColumn fx:id="mapTypeColumn" prefWidth="81.60009765625" text="Map Type" />
            <TableColumn fx:id="itemTypeColumn" prefWidth="100.7999267578125" text="Item Type" />
            <TableColumn fx:id="vehicleTypeColumn" prefWidth="142.4000244140625" text="Vehicle Type" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
